#!/usr/bin/env bash
#

source $HOME/.bash_styles

string="oO08 iIlL1 g9qCGQ ~-+=>\'\""

echo "$string ""default"
echo "${bold}""$string "bold "$default"
echo "${italic}""$string "italic "$default"
echo "${under}""$string "under "$eunder"
echo "${alt}""$string "alt "$default"
echo "${dim}""$string "dim "$default"

# EOF
